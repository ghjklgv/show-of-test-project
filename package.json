{
  "name": "cedd-geo-lib",
  "version": "1.27.0",
  "private": true,
  "dependencies": {
    "@types/enzyme-adapter-react-16": "1.0.4",
    "@types/heatmap.js": "2.0.36",
    "@types/jest": "24.0.6",
    "@types/node": "11.11.0",
    "@types/react-dom": "16.8.4",
    "@types/react-infinite-scroller": "1.2.1",
    "@types/react-router-dom": "4.3.1",
    "antd": "3.19.1",
    "axios": "0.18.0",
    "heatmap.js": "2.0.5",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-router-dom": "4.3.1",
    "react-scripts-ts": "3.1.0",
    "react-test-renderer": "16.8.2",
    "tslint-config-standard": "8.0.1"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "start:new": "sh -ac '. ./.env.development; npm-run-all -p watch:css start'",
    "start:dev": "sh -ac '. ./.env.development; react-scripts-ts start'",
    "watch:css": "npm run build:css && node-sass src/styles/sass/ -o src/styles/css/ --watch --recursive",
    "build": "react-scripts-ts build",
    "build:dev": "sh -ac '. ./.env.development; react-scripts-ts build'",
    "build:css": "node-sass src/styles/sass/ -o src/styles/css/",
    "build:release": "sh -ac '. ./.env.release; react-scripts-ts build'",
    "test": "react-scripts-ts test --env=jsdom",
    "testCov:dev": "sh -ac '. ./.env.development; react-scripts-ts test --coverage --updateSnapshot --env=jsdom'",
    "testCov": "react-scripts-ts test --coverage  --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "ts-jest": "21.1.0",
    "@types/enzyme": "3.1.18",
    "enzyme": "3.9.0",
    "enzyme-to-json": "3.3.5",
    "enzyme-adapter-react-16": "1.9.1",
    "@types/node-sass": "4.11.0",
    "awesome-typescript-loader": "5.2.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "jest": "24.1.0",
    "npm-run-all": "4.1.5",
    "source-map-loader": "0.2.3",
    "typescript": "3.2.2"
  }
}
